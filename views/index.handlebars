</p>
<center><div id="paypal-button"></div>
</center>
        <h5>Project Ideas</h5>
        <ul id="example-list" class="list-group">
          {{#each examples}}{{#unless this.members_only}}
          <li style="line-height:1.5" data-id="{{this.id}}" class="list-group-item">
            <div>
              <a href="ideas/{{this.id}}">{{this.title}}</a>
              <br>
              <span style="font-size:12px"> {{this.description}}<br> <b>Technologies used:</b> {{this.tech_used}}
                {{!-- <div>
                  <b>User Stories:</b>
                  {{#each userstories}}
                  <ul>
                    <li>{{story}}</li>
                  </ul>
                  {{/each}}
                </div> --}}
              </span>
            </div>

          </li>
          {{/unless}}{{/each}}
        </ul>
      </div>
    </div>
  </div>
